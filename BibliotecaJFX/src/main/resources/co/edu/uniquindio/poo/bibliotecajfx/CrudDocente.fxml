<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.bibliotecajfx.viewController.DocenteViewController">
    <children>
        <Pane prefHeight="400.0" prefWidth="602.0">
            <children>
                <Label layoutX="251.0" layoutY="14.0" prefHeight="35.0" prefWidth="100.0" style="-fx-font-size: 24px; -fx-font-weight: bold;" text="Docente" />
                <Button fx:id="btnVerHistorialPrestamosDocente" layoutX="33.0" layoutY="188.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="152.0" text="Ver Historial Prestamos" onAction="#onOpenCrudVerHistorialPrestamosDocente"/>
                <Button fx:id="btnVerLibrosDocente" layoutX="225.0" layoutY="185.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="152.0" text="Ver Libros" onAction="#onOpenCrudVerLibrosDocente"/>
