                <Label layoutX="181.0" layoutY="110.0" text="Seleccione que desea realizar:">
                    <font>
                        <Font name="Times New Roman" size="20.0" />
                    </font>
                </Label>
                <Button fx:id="btnCerrarSesionDocente" layoutX="255.0" layoutY="302.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="92.0" text="Cerrar Sesion" onAction="#onCerrarSesionDocente" />
